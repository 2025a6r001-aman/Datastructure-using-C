#include<stdio.h>
#include<math.h>
int main(){
    int num;
    printf("Enter Binary Number: ");
    scanf("%d", &num);
    int decNum=0, pos=0;
    while(num != 0){
         int rem = num % 10;
        decNum += rem * pow(2, pos);
        pos++;
        num = num /10;

    }
    printf("Decimal = %d", decNum);
    return 0;
}
